{
    "manifest_version": 3,
    "name": "Google Index Bot",
    "version": "1.0",
    "description": "Automatically clicks submit buttons on any website.",
    "permissions": ["tabs", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "action": {
      "default_popup": "index.html"
    }
  }
  